#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/@pandacss+dev@0.24.2_typescript@5.4.0-dev.20240111/node_modules/@pandacss/dev/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/@pandacss+dev@0.24.2_typescript@5.4.0-dev.20240111/node_modules/@pandacss/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/@pandacss+dev@0.24.2_typescript@5.4.0-dev.20240111/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/@pandacss+dev@0.24.2_typescript@5.4.0-dev.20240111/node_modules/@pandacss/dev/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/@pandacss+dev@0.24.2_typescript@5.4.0-dev.20240111/node_modules/@pandacss/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/@pandacss+dev@0.24.2_typescript@5.4.0-dev.20240111/node_modules:/Users/yuri/Sites/polkadot/nft-renderer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@pandacss/dev/bin.js" "$@"
else
  exec node  "$basedir/../@pandacss/dev/bin.js" "$@"
fi
