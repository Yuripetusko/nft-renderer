{
  "extends": [
    "plugin:@nx/react",
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {
        "@typescript-eslint/no-inferrable-types": [
          "off"
        ],
        "@typescript-eslint/no-empty-interface": [
          "warn"
        ],
        "@typescript-eslint/no-empty-function": "warn",
        "react/no-unescaped-entities": [
          "warn"
        ]
      }
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    }
  ]
}
